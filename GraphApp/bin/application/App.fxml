<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AppController">
   <children>
      <Button fx:id="button" layoutX="483.0" layoutY="85.0" mnemonicParsing="false" onAction="#browseFile" prefHeight="31.0" prefWidth="90.0" text="Browse" />
      <TextField fx:id="input" layoutX="30.0" layoutY="85.0" prefHeight="31.0" prefWidth="420.0" />
      <Label fx:id="label" layoutX="254.0" layoutY="14.0" text="Graph App">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="456.0" layoutY="282.0" text="by Thiago Baronio Trevisan">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <RadioButton fx:id="adjacencyMatrix" layoutX="30.0" layoutY="126.0" mnemonicParsing="false" text="Adjacency Matrix">
         <toggleGroup>
            <ToggleGroup fx:id="group" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="edgeList" layoutX="230.0" layoutY="126.0" mnemonicParsing="false" text="Edge List" toggleGroup="$group" />
      <Button fx:id="start" layoutX="250.0" layoutY="206.0" mnemonicParsing="false" onAction="#play" prefHeight="50.0" prefWidth="100.0" text="Start" />
      <Label fx:id="result" layoutX="380.0" layoutY="126.0" prefWidth="100.0" text="Waiting..." />
      <RadioButton fx:id="none" layoutX="30.0" layoutY="166.0" mnemonicParsing="false" text="None" />
      <RadioButton fx:id="fr" layoutX="134.0" layoutY="166.0" mnemonicParsing="false" text="Fruchterman-Reingold" />
      <RadioButton fx:id="walshaw" layoutX="350.0" layoutY="166.0" mnemonicParsing="false" text="Walshaw" />
      <Label fx:id="save" layoutX="30.0" layoutY="231.0" prefWidth="100.0" />
   </children>
</AnchorPane>
